# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001

# Application Configuration
NEXT_PUBLIC_APP_NAME=AI Simulation Platform
NEXT_PUBLIC_APP_URL=http://localhost:3000

# API Keys for LLM providers
# You need at least one of these to use the natural language to SQL feature
ANTHROPIC_API_KEY=your-anthropic-api-key-here
OPENAI_API_KEY=your-openai-api-key-here
CLAUDE_CODE_PATH=/path/to/claude

# Azure AI Agent Configuration (optional - enables agentic text-to-SQL)
AZURE_AI_PROJECT_ENDPOINT=https://your-project.services.ai.azure.com/api/projects/proj-default
AZURE_AI_MODEL_DEPLOYMENT_NAME=gpt-4o
AZURE_AI_AGENT_NAME=text-to-sql-agent

# Database Configuration (same as backend)
DATABASE_URL=postgresql://user:password@host:5432/database

# Node Environment
NODE_ENV=development

# Test user credentials (for development only)
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=test1234
